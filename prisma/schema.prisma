generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Fractal {
  id        Int       @id @default(autoincrement())
  createdAt DateTime  @default(now())
  authorId  String
  form      String
  color     String
  @@index([authorId])
}